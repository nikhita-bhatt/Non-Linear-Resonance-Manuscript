                (h_ex_rand[osc] - y[0] + c1*sigmoid(y[0]) - c2*sigmoid(y[1]))*tau_ex,
                (h_in_rand[osc] - y[1] + c3*sigmoid(y[0]) - c4*sigmoid(y[1]))*tau_in
        )
        
    else:
        # Separate Variables
        y_ex = y[:-1:2]
        y_in = y[1::2]

        dy_ex, dy_in = zeros(N), zeros(N)
        dydt = zeros(2*N)

        for osc in arange(N):
            
